<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="bootstrap-4.4.1-dist/css/bootstrap.css">
    <link rel="stylesheet" href="css/style6.css">
    <title>Updateseats</title>
</head>
<body>
  
    <div class="container-fluid " style="   background-color: #eee;  ">
        <div class="heading" style="font-size: 60px;
        padding-left: 20%;
        padding-right: 10%;">
            Student Councelling
        </div>
    </div>
    <nav class="navbar navbar-light bg-light">
        <form class="form-inline" >
            <a class="nav-link " href="/Admin">Home</a>
            <a class="nav-link " href="/Update">Update</a>
            <a class="nav-link " href="/logout">Logout</a>
        </form>
    </nav>
    <div class="col">

        <div class="container-fluid ">

            <% if(message == "Success") { %>
                <div class="alert alert-success">
                    Succesfully Updated
                </div>
        
                <% } %>



            <div >
                
                    <form action="/Updateseats/<%= list.ID %>" method="POST" class="">
                    <div class="form-group">
                        <label for="Institute">Institute Name</label>
   
    <input type="text" class="form-control" name="institute" value= '<%= list.Institute %>' style=" width:400px !important;" >
                    </div>
                    <div class="form-group">
                        <label for="Course">Course</label>
                        <input type="text" class="form-control" name="course" value=' <%= list.Course %> '  style=" width:400px !important;">
                    </div>
                    <div class="form-group">
                        <label for="seats">Seats</label>
                        <input type="text" class="form-control" name="seats" value='<%= list.Seats %> '  style=" width:400px !important;">
                    </div>
                    <input id="btn-register" type="submit" class="btn btn-primary" value="Submit">
                    </form>
            </div>
        </div>


    </div>
    

    


</body>
</html>

